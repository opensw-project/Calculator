def ascii_calculator():
    숫자1 = int(input("첫 번째 숫자를 입력하세요: "))
    연산자 = input("연산자를 입력하세요 (+ 또는 -): ")
    숫자2 = int(input("두 번째 숫자를 입력하세요: "))

    아스키_숫자1 = ord(str(숫자1))
    아스키_숫자2 = ord(str(숫자2))

    if 연산자 == '+':
        결과 = 아스키_숫자1 + 아스키_숫자2
    elif 연산자 == '-':
        결과 = 아스키_숫자1 - 아스키_숫자2
    else:
        결과 = "잘못된 연산자입니다."

    print("결과:", 결과)


# 계산기 실행
ascii_calculator()
