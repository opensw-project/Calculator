def calculate_cubic_function(a, b, c, d, x):
    result = a * x**3 + b * x**2 + c * x + d
    return result

a = float(input("a 값을 입력하세요: "))
b = float(input("b 값을 입력하세요: "))
c = float(input("c 값을 입력하세요: "))
d = float(input("d 값을 입력하세요: "))

x = float(input("x 값을 입력하세요: "))

result = calculate_cubic_function(a, b, c, d, x)

print("함수 값은:", result)
