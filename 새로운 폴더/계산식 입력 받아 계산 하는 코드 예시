def 계산기(숫자1, 연산자, 숫자2):
    if 연산자 == '+':
        결과 = 숫자1 + 숫자2
    elif 연산자 == '-':
        결과 = 숫자1 - 숫자2
    elif 연산자 == '*':
        결과 = 숫자1 * 숫자2
    elif 연산자 == '/':
        결과 = 숫자1 / 숫자2
    else:
        결과 = "잘못된 연산자입니다."
    return 결과

# 사용 예시
결과 = 계산기(5, '+', 3)  # 덧셈: 5 + 3
print(결과)  # 출력: 8

결과 = 계산기(10, '*', 2)  # 곱셈: 10 * 2
print(결과)  # 출력: 20

결과 = 계산기(7, '&', 4)  # 잘못된 연산자 사용
print(결과)  # 출력: 잘못된 연산자입니다.
