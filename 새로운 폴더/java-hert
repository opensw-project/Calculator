public class HeartOutputExample {

    public static void main(String[] args) {
        String heart = 
            "  ***     ***  \n" +
            " *****   ***** \n" +
            "******* *******\n" +
            " ************* \n" +
            "  ***********  \n" +
            "   *********   \n" +
            "    *******    \n" +
            "     *****     \n" +
            "      ***      ";

        System.out.println(heart);
    }
}
