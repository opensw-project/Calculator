def is_prime(n):  
    if n <= 1:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True

def find_largest_prime(N):
    largest_prime = None
    for num in range(1, N + 1):
        if is_prime(num):
            largest_prime = num
    return largest_prime

N = 100
result = find_largest_prime(N)
print(f"The largest prime number between 1 and {N} is: {result}")
